{
  "active_pane_modifiers": {
    "inactive_opacity": 0.7
  },
  "assistant": {
    "default_model": {
      "provider": "anthropic",
      "model": "claude-3-5-sonnet-latest"
    },
    "version": "2"
  },
  "auto_install_extensions": {
    "docker-compose": true,
    "dockerfile": true,
    "html": true,
    "justfile": true,
    "toml": true
  },
  "buffer_font_size": 16,
  "buffer_line_height": "standard",
  "chat_panel": {
    "button": false
  },
  "collaboration_panel": {
    "button": false
  },
  "cursor_blink": false,
  "cursor_shape": "block",
  "indent_guides": {
    "enabled": true,
    "indentation": 2,
    "coloring": "indent_aware"
  },
  "lsp": {
    "gopls": {
      "gofumpt": true,
      "initialization_options": {
        "gofumpt": true
      }
    },
    "pyright": {
      "settings": {
        "python": {
          "pythonPath": ".venv/bin/python"
        }
      }
    }
  },
  "preview_tabs": {
    "enabled": false,
    "enable_preview_from_code_navigation": false,
    "enable_preview_from_file_finder": false
  },
  "project_panel": {
    "auto_fold_dirs": false
  },
  "relative_line_numbers": true,
  "scrollbar": {
    "show": "never"
  },
  "tab_bar": {
    "show": false
  },
  "telemetry": {
    "diagnostics": false,
    "metrics": false
  },
  "terminal": {
    "button": false,
    "copy_on_select": true,
    "font_family": "JetBrainsMono Nerd Font Mono",
    "toolbar": {
      "breadcrumbs": false
    }
  },
  "theme": "Nordfox",
  "ui_font_size": 16,
  "use_system_path_prompts": false,
  "vim_mode": true
}
